@prefix : <http://example.org/pharma#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology declaration
: a owl:Ontology ;
  rdfs:label "Drug Development Pipeline Ontology" ;
  rdfs:comment "Ontology for pharmaceutical drug development from ideation to commercialization" .

# Classes
:Compound a owl:Class ;
  rdfs:label "Compound" ;
  rdfs:comment "Chemical compounds and drug candidates" .

:Target a owl:Class ;
  rdfs:label "Target" ;
  rdfs:comment "Biological targets for drug development" .

:PreclinicalStudy a owl:Class ;
  rdfs:label "Preclinical Study" ;
  rdfs:comment "Preclinical research and testing studies" .

:ClinicalTrial a owl:Class ;
  rdfs:label "Clinical Trial" ;
  rdfs:comment "Clinical trial studies across all phases" .

:RegulatorySubmission a owl:Class ;
  rdfs:label "Regulatory Submission" ;
  rdfs:comment "Regulatory submissions and approvals" .

:Manufacturing a owl:Class ;
  rdfs:label "Manufacturing" ;
  rdfs:comment "Drug manufacturing and production" .

:Commercialization a owl:Class ;
  rdfs:label "Commercialization" ;
  rdfs:comment "Drug commercialization and market performance" .

# Compound Properties
:compoundId a owl:DatatypeProperty ;
  rdfs:label "Compound ID" ;
  rdfs:comment "Unique compound identifier" ;
  rdfs:domain :Compound ;
  rdfs:range xsd:string .

:compoundName a owl:DatatypeProperty ;
  rdfs:label "Compound Name" ;
  rdfs:comment "Chemical name of the compound" ;
  rdfs:domain :Compound ;
  rdfs:range xsd:string .

:molecularFormula a owl:DatatypeProperty ;
  rdfs:label "Molecular Formula" ;
  rdfs:comment "Molecular formula of the compound" ;
  rdfs:domain :Compound ;
  rdfs:range xsd:string .

:therapeuticArea a owl:DatatypeProperty ;
  rdfs:label "Therapeutic Area" ;
  rdfs:comment "Target therapeutic area" ;
  rdfs:domain :Compound ;
  rdfs:range xsd:string .

:discoveryMethod a owl:DatatypeProperty ;
  rdfs:label "Discovery Method" ;
  rdfs:comment "Method used for compound discovery" ;
  rdfs:domain :Compound ;
  rdfs:range xsd:string .

:compoundStatus a owl:DatatypeProperty ;
  rdfs:label "Compound Status" ;
  rdfs:comment "Current development status" ;
  rdfs:domain :Compound ;
  rdfs:range xsd:string .

# Target Properties
:targetId a owl:DatatypeProperty ;
  rdfs:label "Target ID" ;
  rdfs:comment "Unique target identifier" ;
  rdfs:domain :Target ;
  rdfs:range xsd:string .

:targetName a owl:DatatypeProperty ;
  rdfs:label "Target Name" ;
  rdfs:comment "Name of the biological target" ;
  rdfs:domain :Target ;
  rdfs:range xsd:string .

:targetType a owl:DatatypeProperty ;
  rdfs:label "Target Type" ;
  rdfs:comment "Type of biological target (protein, enzyme, receptor)" ;
  rdfs:domain :Target ;
  rdfs:range xsd:string .

:diseaseIndication a owl:DatatypeProperty ;
  rdfs:label "Disease Indication" ;
  rdfs:comment "Associated disease indication" ;
  rdfs:domain :Target ;
  rdfs:range xsd:string .

:validationStatus a owl:DatatypeProperty ;
  rdfs:label "Validation Status" ;
  rdfs:comment "Target validation status" ;
  rdfs:domain :Target ;
  rdfs:range xsd:string .

# Preclinical Study Properties
:studyId a owl:DatatypeProperty ;
  rdfs:label "Study ID" ;
  rdfs:comment "Unique preclinical study identifier" ;
