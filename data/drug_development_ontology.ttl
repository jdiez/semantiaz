  rdfs:range :ClinicalTrial .

:hasSubmission a owl:ObjectProperty ;
  rdfs:label "Has Submission" ;
  rdfs:comment "Clinical trial has regulatory submission" ;
  rdfs:domain :ClinicalTrial ;
  rdfs:range :RegulatorySubmission .

:isManufactured a owl:ObjectProperty ;
  rdfs:label "Is Manufactured" ;
