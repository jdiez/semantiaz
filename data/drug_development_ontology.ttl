:targetsTarget a owl:ObjectProperty ;
  rdfs:label "Targets Target" ;
  rdfs:comment "Compound targets a biological target" ;
  rdfs:domain :Compound ;
  rdfs:range :Target .

:undergoesStudy a owl:ObjectProperty ;
  rdfs:label "Undergoes Study" ;
  rdfs:comment "Compound undergoes preclinical study" ;
