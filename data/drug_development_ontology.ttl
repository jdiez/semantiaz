  rdfs:range xsd:string .

:studyType a owl:DatatypeProperty ;
  rdfs:label "Study Type" ;
  rdfs:comment "Type of preclinical study" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:string .

:studyPhase a owl:DatatypeProperty ;
  rdfs:label "Study Phase" ;
  rdfs:comment "Preclinical phase (in vitro, in vivo, toxicology)" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:string .

:studyStatus a owl:DatatypeProperty ;
  rdfs:label "Study Status" ;
  rdfs:comment "Current study status" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:string .

:species a owl:DatatypeProperty ;
  rdfs:label "Species" ;
  rdfs:comment "Test species for in vivo studies" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:string .

:studyStartDate a owl:DatatypeProperty ;
  rdfs:label "Study Start Date" ;
  rdfs:comment "Study start date" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:date .

:studyEndDate a owl:DatatypeProperty ;
  rdfs:label "Study End Date" ;
  rdfs:comment "Study completion date" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:date .

:efficacyScore a owl:DatatypeProperty ;
  rdfs:label "Efficacy Score" ;
  rdfs:comment "Efficacy measurement score" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:decimal .

:safetyScore a owl:DatatypeProperty ;
  rdfs:label "Safety Score" ;
  rdfs:comment "Safety assessment score" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:decimal .

:studyCost a owl:DatatypeProperty ;
  rdfs:label "Study Cost" ;
  rdfs:comment "Cost of preclinical study" ;
  rdfs:domain :PreclinicalStudy ;
  rdfs:range xsd:decimal .

# Clinical Trial Properties
:trialId a owl:DatatypeProperty ;
  rdfs:label "Trial ID" ;
  rdfs:comment "Unique clinical trial identifier" ;
  rdfs:domain :ClinicalTrial ;
  rdfs:range xsd:string .

:trialPhase a owl:DatatypeProperty ;
