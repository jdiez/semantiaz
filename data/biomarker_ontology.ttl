@prefix : <http://example.org/biomarker#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Ontology declaration
: a owl:Ontology ;
  rdfs:label "Biomarker Characterization Ontology" ;
  rdfs:comment "Ontology for biomarker characterization from molecular studies and clinical survival trials" .

# Classes
:Biomarker a owl:Class ;
  rdfs:label "Biomarker" ;
  rdfs:comment "Molecular biomarkers and their characteristics" .

:MolecularAssay a owl:Class ;
  rdfs:label "Molecular Assay" ;
  rdfs:comment "Laboratory assays for biomarker measurement" .

:PatientSample a owl:Class ;
  rdfs:label "Patient Sample" ;
  rdfs:comment "Patient samples collected for biomarker analysis" .

:BiomarkerMeasurement a owl:Class ;
  rdfs:label "Biomarker Measurement" ;
  rdfs:comment "Quantitative biomarker measurements from patient samples" .

:ClinicalStudy a owl:Class ;
  rdfs:label "Clinical Study" ;
  rdfs:comment "Clinical studies investigating biomarker-outcome relationships" .

:PatientOutcome a owl:Class ;
  rdfs:label "Patient Outcome" ;
  rdfs:comment "Clinical outcomes and survival data for patients" .

:BiomarkerAssociation a owl:Class ;
  rdfs:label "Biomarker Association" ;
  rdfs:comment "Statistical associations between biomarkers and clinical outcomes" .

# Biomarker Properties
:biomarkerId a owl:DatatypeProperty ;
  rdfs:label "Biomarker ID" ;
  rdfs:comment "Unique biomarker identifier" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:biomarkerName a owl:DatatypeProperty ;
  rdfs:label "Biomarker Name" ;
  rdfs:comment "Official biomarker name" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:biomarkerType a owl:DatatypeProperty ;
  rdfs:label "Biomarker Type" ;
  rdfs:comment "Type of biomarker (protein, gene, metabolite, miRNA)" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:molecularPathway a owl:DatatypeProperty ;
  rdfs:label "Molecular Pathway" ;
  rdfs:comment "Associated molecular pathway" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:tissueSpecificity a owl:DatatypeProperty ;
  rdfs:label "Tissue Specificity" ;
  rdfs:comment "Tissue or organ specificity" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:biomarkerClass a owl:DatatypeProperty ;
  rdfs:label "Biomarker Class" ;
  rdfs:comment "Functional classification (diagnostic, prognostic, predictive)" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:validationStatus a owl:DatatypeProperty ;
  rdfs:label "Validation Status" ;
  rdfs:comment "Clinical validation status" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

:detectionMethod a owl:DatatypeProperty ;
  rdfs:label "Detection Method" ;
  rdfs:comment "Primary detection/measurement method" ;
  rdfs:domain :Biomarker ;
  rdfs:range xsd:string .

# Molecular Assay Properties
:assayId a owl:DatatypeProperty ;
  rdfs:label "Assay ID" ;
  rdfs:comment "Unique assay identifier" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:string .

:assayName a owl:DatatypeProperty ;
  rdfs:label "Assay Name" ;
  rdfs:comment "Name of the assay" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:string .

:assayType a owl:DatatypeProperty ;
  rdfs:label "Assay Type" ;
  rdfs:comment "Type of assay (ELISA, PCR, NGS, Mass Spec)" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:string .

:platform a owl:DatatypeProperty ;
  rdfs:label "Platform" ;
  rdfs:comment "Instrument platform used" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:string .

:sampleType a owl:DatatypeProperty ;
  rdfs:label "Sample Type" ;
  rdfs:comment "Required sample type (blood, tissue, urine)" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:string .

:assayStatus a owl:DatatypeProperty ;
  rdfs:label "Assay Status" ;
  rdfs:comment "Current assay status" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:string .

:sensitivity a owl:DatatypeProperty ;
  rdfs:label "Sensitivity" ;
  rdfs:comment "Assay sensitivity percentage" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:decimal .

:specificity a owl:DatatypeProperty ;
  rdfs:label "Specificity" ;
  rdfs:comment "Assay specificity percentage" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:decimal .

:detectionLimit a owl:DatatypeProperty ;
  rdfs:label "Detection Limit" ;
  rdfs:comment "Lower limit of detection" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:decimal .

:assayCost a owl:DatatypeProperty ;
  rdfs:label "Assay Cost" ;
  rdfs:comment "Cost per assay" ;
  rdfs:domain :MolecularAssay ;
  rdfs:range xsd:decimal .

# Patient Sample Properties
:sampleId a owl:DatatypeProperty ;
  rdfs:label "Sample ID" ;
  rdfs:comment "Unique sample identifier" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:string .

:patientId a owl:DatatypeProperty ;
  rdfs:label "Patient ID" ;
  rdfs:comment "Patient identifier" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:string .

:collectionSite a owl:DatatypeProperty ;
  rdfs:label "Collection Site" ;
  rdfs:comment "Anatomical collection site" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:string .

:sampleStatus a owl:DatatypeProperty ;
  rdfs:label "Sample Status" ;
  rdfs:comment "Current sample status" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:string .

:storageCondition a owl:DatatypeProperty ;
  rdfs:label "Storage Condition" ;
  rdfs:comment "Sample storage conditions" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:string .

:collectionDate a owl:DatatypeProperty ;
  rdfs:label "Collection Date" ;
  rdfs:comment "Sample collection date" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:date .

:processingDate a owl:DatatypeProperty ;
  rdfs:label "Processing Date" ;
  rdfs:comment "Sample processing date" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:date .

:sampleVolume a owl:DatatypeProperty ;
  rdfs:label "Sample Volume" ;
  rdfs:comment "Sample volume in mL" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:decimal .

:sampleQualityScore a owl:DatatypeProperty ;
  rdfs:label "Sample Quality Score" ;
  rdfs:comment "Sample quality assessment score" ;
  rdfs:domain :PatientSample ;
  rdfs:range xsd:decimal .

# Biomarker Measurement Properties
:measurementId a owl:DatatypeProperty ;
  rdfs:label "Measurement ID" ;
  rdfs:comment "Unique measurement identifier" ;
  rdfs:domain :BiomarkerMeasurement ;
  rdfs:range xsd:string .

:measurementStatus a owl:DatatypeProperty ;
  rdfs:label "Measurement Status" ;
  rdfs:comment "Measurement status (completed, pending, failed)" ;
  rdfs:domain :BiomarkerMeasurement ;
  rdfs:range xsd:string .

:measurementUnit a owl:DatatypeProperty ;
  rdfs:label "Measurement Unit" ;
  rdfs:comment "Unit of measurement" ;
  rdfs:domain :BiomarkerMeasurement ;
  rdfs:range xsd:string .

:referenceRange a owl:DatatypeProperty ;
  rdfs:label "Reference Range" ;
  rdfs:comment "Normal reference range" ;
  rdfs:domain :BiomarkerMeasurement ;
  rdfs:range xsd:string .

:measurementDate a owl:DatatypeProperty ;
  rdfs:label "Measurement Date" ;
  rdfs:comment "Date of measurement" ;
  rdfs:domain :BiomarkerMeasurement ;
