  rdfs:comment "Type of clinical outcome (survival, response, toxicity)" ;
  rdfs:domain :PatientOutcome ;
  rdfs:range xsd:string .

:outcomeStatus a owl:DatatypeProperty ;
  rdfs:label "Outcome Status" ;
  rdfs:comment "Outcome status (event, censored, ongoing)" ;
  rdfs:domain :PatientOutcome ;
  rdfs:range xsd:string .

:treatmentResponse a owl:DatatypeProperty ;
  rdfs:label "Treatment Response" ;
  rdfs:comment "Treatment response category" ;
  rdfs:domain :PatientOutcome ;
  rdfs:range xsd:string .

:diseaseProgression a owl:DatatypeProperty ;
  rdfs:label "Disease Progression" ;
  rdfs:comment "Disease progression status" ;
