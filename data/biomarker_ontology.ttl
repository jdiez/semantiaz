# Object Properties (Relationships)
:measuredBy a owl:ObjectProperty ;
  rdfs:label "Measured By" ;
  rdfs:comment "Biomarker is measured by molecular assay" ;
  rdfs:domain :Biomarker ;
  rdfs:range :MolecularAssay .

:belongsToPatient a owl:ObjectProperty ;
  rdfs:label "Belongs To Patient" ;
  rdfs:comment "Sample belongs to patient" ;
  rdfs:domain :PatientSample ;
  rdfs:range :PatientOutcome .

:measurementFromSample a owl:ObjectProperty ;
  rdfs:label "Measurement From Sample" ;
  rdfs:comment "Measurement is from patient sample" ;
  rdfs:domain :BiomarkerMeasurement ;
  rdfs:range :PatientSample .

:measurementOfBiomarker a owl:ObjectProperty ;
  rdfs:label "Measurement Of Biomarker" ;
  rdfs:comment "Measurement is of specific biomarker" ;
  rdfs:domain :BiomarkerMeasurement ;
  rdfs:range :Biomarker .

:outcomeInStudy a owl:ObjectProperty ;
  rdfs:label "Outcome In Study" ;
  rdfs:comment "Patient outcome is part of clinical study" ;
  rdfs:domain :PatientOutcome ;
  rdfs:range :ClinicalStudy .

:associationWithBiomarker a owl:ObjectProperty ;
  rdfs:label "Association With Biomarker" ;
  rdfs:comment "Statistical association involves biomarker" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range :Biomarker .

:associationWithOutcome a owl:ObjectProperty ;
  rdfs:label "Association With Outcome" ;
  rdfs:comment "Statistical association involves patient outcome" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range :PatientOutcome .
