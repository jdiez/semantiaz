  rdfs:domain :BiomarkerAssociation ;
  rdfs:range xsd:string .

:associationType a owl:DatatypeProperty ;
  rdfs:label "Association Type" ;
  rdfs:comment "Type of statistical association" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range xsd:string .

:analysisMethod a owl:DatatypeProperty ;
  rdfs:label "Analysis Method" ;
  rdfs:comment "Statistical analysis method used" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range xsd:string .

:significanceLevel a owl:DatatypeProperty ;
  rdfs:label "Significance Level" ;
  rdfs:comment "Statistical significance level" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range xsd:string .

:correlationCoefficient a owl:DatatypeProperty ;
  rdfs:label "Correlation Coefficient" ;
  rdfs:comment "Correlation coefficient" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range xsd:decimal .

:oddsRatio a owl:DatatypeProperty ;
  rdfs:label "Odds Ratio" ;
  rdfs:comment "Odds ratio for binary outcomes" ;
  rdfs:domain :BiomarkerAssociation ;
  rdfs:range xsd:decimal .

:aucScore a owl:DatatypeProperty ;
  rdfs:label "AUC Score" ;
